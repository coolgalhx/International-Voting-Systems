<Window x:Class="International_Voting_Systems.ManageVoterAccounts"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:International_Voting_Systems"
        mc:Ignorable="d"
        Title="ManageVoterAccounts" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="datagridvoters"
          ItemsSource="{Binding Voters}"
          AutoGenerateColumns="True"
          IsReadOnly="True" Margin="0,0,182,10" />
        <Button Content="Button" HorizontalAlignment="Left" Margin="654,102,0,0" VerticalAlignment="Top" Height="41" Width="90" Click="Button_Click"/>

    </Grid>
</Window>
