<Window x:Class="International_Voting_Systems.VoterRegisteration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:International_Voting_Systems"
        mc:Ignorable="d"
        Title="VoterRegisteration" Height="450" Width="800">
    <Grid>
        <TextBox Name="txtname" HorizontalAlignment="Left" Margin="228,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="31"/>
        <TextBox Name="txtage" HorizontalAlignment="Left" Margin="59,166,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="31"/>
        <TextBox Name="txtdob" HorizontalAlignment="Left" Margin="400,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="31"/>
        <TextBox Name="txtemail" HorizontalAlignment="Left" Margin="59,50,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="31"/>
        <TextBox Name="txtcontactnumber" HorizontalAlignment="Left" Margin="59,110,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="32"/>
        <TextBox Name="txtvoterid" HorizontalAlignment="Left" Margin="228,165,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="32"/>
        <TextBox Name="txtgender" HorizontalAlignment="Left" Margin="228,110,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="138" Height="31"/>
        <Button Name="btnregistervoter" HorizontalAlignment="Left" Margin="400,110,0,0" VerticalAlignment="Top" Height="48" Width="138" Click="btnregistervoter_Click"/>
        <Label Name="lblname" HorizontalAlignment="Left" Margin="279,326,0,0" VerticalAlignment="Top" Height="29" Width="56" Content="Name"/>
        <ComboBox x:Name="TranslationCombo"
          SelectionChanged="TranslatorCombo_SelectionChanged"
          HorizontalAlignment="Left"
          Margin="658,182,0,0"
          VerticalAlignment="Top"
          Width="120">

            <ComboBoxItem Content="French" Tag="fr"/>
            <ComboBoxItem Content="German" Tag="de"/>
            <ComboBoxItem Content="Spanish" Tag="es"/>

        </ComboBox>


    </Grid>
</Window>
